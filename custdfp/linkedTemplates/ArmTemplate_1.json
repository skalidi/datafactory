						"source1 derive(Address = @(City=cust_city,",
						"          State=cust_state,",
						"          Country=cust_country)) ~> derivedColumn1",
						"derivedColumn1 aggregate(groupBy(cust_no,",
						"          cust_name),",
						"     Address = collect(Address)) ~> aggregate1",
						"aggregate1 sink(allowSchemaDrift: true,",
